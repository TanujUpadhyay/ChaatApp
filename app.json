{
  "expo": {
    "name": "ChatApp",
    "slug": "ChatApp",
    "platforms": ["ios", "android", "web"],
    "android": {
      "package": "com.alpghacode.chatapp",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.alpghacode.chatapp"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AlphacCode.png",
    "splash": {
      "image": "./assets/AlphacCode.png",
      "resizeMode": "contain",
      "backgroundColor": "#393e46"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
